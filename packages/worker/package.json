{
  "ava": {
    "failFast": false,
    "files": [
      "dist/tests/**/*.js"
    ],
    "timeout": "1m"
  },
  "dependencies": {
    "@azure/functions": "^1.0.3",
    "@online-service/utils": "^1.0.0",
    "lodash": "^4.17.15",
    "serverless-azure-functions": "^1.0.0"
  },
  "devDependencies": {
    "@hint/utils": "^5.0.1",
    "@types/lodash": "^4.14.139",
    "@types/node": "^12.7.7",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "ava": "^2.4.0",
    "cpx": "^1.5.0",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eventemitter2": "^5.0.1",
    "globby": "^10.0.1",
    "hint": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.4.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "main": "./dist/src/index.js",
  "name": "@online-service/worker",
  "private": true,
  "scripts": {
    "ava": "ava",
    "build": "npm run clean && npm-run-all build:*",
    "build:assets": "cpx \"./{src,tests}/**/{!(*.ts),.!(ts)}\" dist",
    "build:ts": "tsc",
    "clean": "rimraf dist",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext ts --ignore-pattern dist .",
    "test": "npm run lint && npm run build && nyc ava",
    "watch:ts": "npm run build:ts -- --watch"
  },
  "version": "0.16.0"
}
